  QueryTimer = Timer.New()
  
  QueryTimer.EventHandler = function()
    TCPClient:Write("< GET ALL >")            ---Get All Data
            if(DebugFunction)then print(DebugFormat("Found Command: "..gsArray[x].."Array Position: "..x))end        
