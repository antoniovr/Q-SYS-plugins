  local gaTextControls = {
    Name = {"MODEL","SERIAL_NUM","FW_VER","IP_ADDR_NET_AUDIO_PRIMARY","IP_SUBNET_NET_AUDIO_PRIMARY","IP_GATEWAY_NET_AUDIO_PRIMARY",
